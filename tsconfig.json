{
    "compilerOptions": {
      "preserveConstEnums": true,
      "strictNullChecks": true,
      "sourceMap": true,
      "target": "es2017",
      "outDir": ".build",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "lib": ["es2017"],
      "resolveJsonModule": true
    },
      "exclude": [
      "node_modules"
    ]
}